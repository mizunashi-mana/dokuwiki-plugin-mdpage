{
    "name": "mizunashi-mana/dokuwiki-plugin-mdpage",
    "type": "dokuwiki-plugin",
    "description": "DokuWiki Markdown Plugin",
    "keywords": [
        "dokuwiki",
        "plugin",
        "markdown"
    ],
    "license": [
        "Apache-2.0",
        "GPL-2.0-or-later"
    ],
    "authors": [
        {
            "name": "Mizunashi Mana",
            "email": "mizunashi-mana@noreply.git"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "cebe/markdown": "^1.2",
        "kirra/markdown-task-lists": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.2",
        "splitbrain/dokuwiki": "dev-stable#release_stable_2018-04-22b",
        "friendsofphp/php-cs-fixer": "^2.13",
        "metasyntactical/composer-plugin-license-check": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "DokuWiki\\": "src/DokuWiki"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dokuwiki\\Test\\": "tests/DokuWiki/Test"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/splitbrain/dokuwiki"
        }
    ],
    "extra": {
        "metasyntactical/composer-plugin-license-check": {
            "whitelist": [],
            "blacklist": []
        }
    }
}
